    "@mdi/js": "^5.1.45",
    "axios": "^0.19.2",
    "clsx": "^1.1.0",
    "express": "^4.17.1",
    "hint.css": "^2.6.0",
    "jss": "^10.1.1",
    "material-ui-chip-input": "^1.1.0",
    "moment": "^2.24.0",
    "query-string": "^6.12.1",
    "react": "^16.13.1",
    "react-cookie": "^4.0.3",
    "react-dom": "^16.13.1",
    "react-join": "^1.1.4",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
